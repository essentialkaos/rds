/var/log/rds/*/redis.log {
  size 10M
  missingok
  rotate 5
  compress
  delaycompress
  notifempty
  create 644 redis redis
}

/var/log/rds/rds.log {
  size 15M
  missingok
  rotate 5
  compress
  delaycompress
  notifempty
  create 640 root root
}

/var/log/rds/rds-sync.log {
  size 15M
  missingok
  rotate 5
  compress
  delaycompress
  notifempty
  create 640 root root
  sharedscripts
  postrotate
    systemctl reload rds-sync.service &> /dev/null || :
  endscript
}
